<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ShiftDiary.Models" assembly="ShiftDiary.Models">

  <class name="ShiftHBM" table="Shift">
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"></column>
      <generator class="identity"></generator>
    </id>
    <property name="Name" not-null="true"></property>
    <property name="ShiftDate" not-null="true"></property>
    <property name="ShiftStart" not-null="false"></property>
    <property name="ShiftEnd" not-null="false"></property>
    <property name="ShiftBreakMins" not-null="false"></property>
  </class>

</hibernate-mapping>
